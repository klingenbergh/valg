    "date-fns": "^3.6.0",
    "debug": "^4.3.7",
    "express": "^4.19.2",
    "helmet": "^8.0.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "zod": "^3.23.8",
    "better-sqlite3": "^9.6.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.10",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.10.3",
    "eslint-plugin-promise": "^6.3.1",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "tsup": "^8.1.0",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
